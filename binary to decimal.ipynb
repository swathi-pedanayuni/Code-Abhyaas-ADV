   "source": [
    "## to convert binary number  to decimal number\n",
    "\n",
    "binary='101010'\n",
    "decimal=0\n",
    "\n",
    "for i in range(len(binary)):\n",
    "    digit=int(binary[len(binary)-1-i])\n",
    "    decimal+=digit*2**i\n",
    "\n",
    "print(\"binary_number:\",binary)\n",
    "print(\"decimal_number:\",decimal)\n",
    "\n",
    "\n",
    "\n"
   ]
