   "source": [
    "#finding maximum subarray sum\n",
    "#kadane's algorithm\n",
    "\n",
    "arr=[-2,-3,4,-1,-2,1,5,-3]\n",
    "sum=0\n",
    "max_sum=0\n",
    "\n",
    "for i in range(len(arr)):\n",
    "    sum+=arr[i]\n",
    "    if sum<0:\n",
    "        sum=0\n",
    "    if sum>max_sum:\n",
    "        max_sum=sum\n",
    "print(\"maximum sum is :\",max_sum)"
   ]
